<main role="main">
  <div class="container">

    <div class="lead">

        <header>
          <!--<a href="/"><img src="https://d343nkq91ijxgw.cloudfront.net/images/logo.svg" id="logo" height="60" width="60"></a>-->
          <span class="glyphicon glyphicon-check logo"></span>
        </header>

        <h1>Finding the best History Report?</h1>
        <h2><em>Each vehicle is different.</em> Compare them side by side.</h2>

        <div class="call-to-action">
          <div class="signup-form">
            <%= render partial: 'vins/form' %>
          </div>
        </div>

        <div id="preview">
          <%= image_tag ('nice-car.png'), height: "160", class: 'pull-left col-lg-offset-1 home-vehicle-left' %>
          <%= image_tag ('audi-r8-new.png'), height: "160", class: 'pull-right home-vehicle-right' %>
        </div>
    </div>
    <div class="report-logo text-center">
      <small class="text-muted">Comparison includes:</small>
      <div class="row">
        <%= image_tag ('carfax-logo.png'), height: '40', width: '180', class: 'carfax-logo pull-left col-lg-offset-1 col-md-offset-1 col-sm-offset-1 col-xs-offset-1' %>
        <%= image_tag ('fastvin-logo.png'), height: '50', class: 'col-lg-offset-2 col-lg-3 col-md-offset-1 col-md-4 col-sm-offset-1 col-sm-4 col-xs-offset-1 col-xs-4' %>
        <%= image_tag ('autocheck-logo.png'), height: '50', class: 'col-lg-offset-1 col-lg-2 col-md-3 col-sm-3 col-xs-3' %>
      </div>
    </div>

  </div>
</main>